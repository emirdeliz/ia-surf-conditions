# OpenWeatherMap API Key
# Get your free API key at: https://openweathermap.org/api
OPENWEATHER_API_KEY=your_openweather_api_key_here

# Llama Configuration
# Local Llama model via Ollama
LLAMA_BASE_URL=http://localhost:11434
LLAMA_MODEL=llama3.2

# Pinecone Configuration
# Get your API key at: https://app.pinecone.io/
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here

# MongoDB Configuration
# Local MongoDB or MongoDB Atlas
MONGODB_URI=mongodb://localhost:27017/surf_conditions
# For MongoDB Atlas: mongodb+srv://username:password@cluster.mongodb.net/surf_conditions

# Optional: Other weather service API keys
# WEATHER_API_KEY=your_weather_api_key
# TIDE_API_KEY=your_tide_api_key
